modules = ["dotnet-7.0", "dotnet-8.0"]
[agent]
expertMode = true

[nix]
channel = "stable-25_05"

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Run Crawler"

[[workflows.workflow]]
name = "Run Crawler"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "dotnet run --project simple_crawler.csproj"

[workflows.workflow.metadata]
outputType = "console"

[deployment]
deploymentTarget = "cloudrun"
